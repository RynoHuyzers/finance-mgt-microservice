  "scripts": {
    "clean": "rimraf dist deploy build cdk/cdk.out cdk/dist && mkdirp deploy",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start":"npx cross-env AWS_SDK_LOAD_CONFIG=true npx nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cdk:build:nest-layer": "cd cdk/src/nest-layer && npx tsc",
    "cdk:build:rest-api-proxy": "cd cdk/src/rest-api/proxy-lambda && npx tsc",
    "cdk:build:rest-api-gw": "cd cdk/src/rest-api/gateway && npx tsc",
    "cdk:build:dynamodb": "cd cdk/src/dynamodb && npx tsc",
    "cdk:synth:nest-layer": "npm run cdk:build:nest-layer && cd cdk/src/nest-layer && rimraf cdk.out dist && npx cdk synth",
    "cdk:synth:rest-api-proxy": "npm run cdk:build:nest-layer && cd cdk/src/nest-layer && rimraf cdk.out dist && npx cdk synth",
    "cdk:deploy:nest-layer": "cd cdk/src/nest-layer && cdk deploy",
    "cdk:deploy:rest-api-proxy": "cd cdk/src/rest-api/proxy-lambda && npx rimraf cdk.out && cdk deploy",
    "cdk:deploy:rest-api-gw": "cd cdk/src/rest-api/gateway && npx rimraf cdk.out && cdk deploy",
    "cdk:deploy:dynamodb": "cd cdk/src/dynamodb && npx rimraf cdk.out && cdk deploy"
  },
